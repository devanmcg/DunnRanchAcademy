@Article{spiess2020,
  author   = {Spiess, Jonathan W. and McGranahan, Devan Allen and Geaumont, Benjamin and Sedivec, Kevin and Lakey, Micayla and Berti, Marisol and Hovick, Torre J. and Limb, Ryan F.},
  journal  = {Rangeland Ecology and Management},
