@Article{haines1988,
  author   = {Haines, Donald A},
  journal  = {National Weather Digest},
  title    = {A lower atmospheric severity index for wildland fires},
  year     = {1988},
  number   = {2},
  pages    = {23--27},
  volume   = {13},
  abstract = {Dry, unstable air increases the probability that wildland fires will become large andlor erratic. This paper describes an atmospheric index for these fires, based on the environmental lapse rate of a layer of air coupled with its moisture content. Inlow-elevation regions ofthe United States, the index is derived from a lapse rate value using 950- to 850-mb temperature differences and a temperature-dew-point spread at 850 mb. At midelevations , the index uses 850- to 700-mb temperature differences and the 850-mb temperature-dew-point spread. In the high-elevation western regions, a similar calculation uses a 700- to 500-mb lapse rate value and the temperature-dew-point spread at 700 mb .},
  language = {en},
}

